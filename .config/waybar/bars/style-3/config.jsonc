    "tray": {
        "spacing": 8
    },

    "idle_inhibitor": {
        "format": "{icon}",
        "start-activated": true,
        "format-icons": {
            "activated": "ÔàÖ",
            "deactivated": "ÔàÑ"
        }
    },

    "clock": {
        "format": "ÔÑ≥ {:%I:%M %p}",
        "format-alt": "{:%R ÔÑ≥ %d¬∑%m¬∑%y}",
        "tooltip-format": "<tt>{calendar}</tt>",
        "calendar": {
            "mode": "month",
            "mode-mon-col": 3,
            "on-scroll": 1,
            "on-click-right": "mode",
            "format": {
                "months": "<span color='#ffead3'><b>{}</b></span>",
                "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
                "today": "<span color='#ff6699'><b>{}</b></span>"
            }
        },
        "actions": {
            "on-click-right": "mode",
            "on-click-forward": "tz_up",
            "on-click-backward": "tz_down",
            "on-scroll-up": "shift_up",
            "on-scroll-down": "shift_down"
        }
    },

    "network": {
        "tooltip": true,
        "format-wifi": "Ôá´ {essid}",
        "format-ethernet": "Û∞àÄ {essid}",
        // "format-wifi": "<span foreground='#99ffdd'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#ffcc66'>ÔÇì {bandwidthUpBytes}</span>",
        // "format-ethernet": "<span foreground='#99ffdd'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#ffcc66'>ÔÇì {bandwidthUpBytes}</span>",
        "tooltip-format": "Network: <big><b>{essid}</b></big>\nSignal strength: <b>{signaldBm}dBm ({signalStrength}%)</b>\nFrequency: <b>{frequency}MHz</b>\nInterface: <b>{ifname}</b>\nIP: <b>{ipaddr}/{cidr}</b>\nGateway: <b>{gwaddr}</b>\nNetmask: <b>{netmask}</b>",
        "format-linked": "Û∞àÄ {ifname} (No IP)",
        "format-disconnected": " Û∞ñ™ ",
        "tooltip-format-disconnected": "Disconnected",
        //"format-alt": "{essid}",
        "format-alt": "<span foreground='#99ffdd'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#ffcc66'>ÔÇì {bandwidthUpBytes}</span>",
        "interval": 2,
        "on-click-right": "~/.config/waybar/scripts/network.py"
    },

    "bluetooth": {
        "format-disabled": "Û∞ÇØ",
        "format-off": "Û∞Ç≤",
        "format-on": "Û∞ÇØ",
        "format-connected": "Û∞ÇØ {num_connections}",
        "format-connected-battery": "Û∞ÇÄÛ∞ÇØ",
        "on-click": "blueberry",
        "on-click-right": "bluetoothctl power off",
        "tooltip-format": "{controller_alias}\n{num_connections} connected",
        "tooltip-format-connected": "connected {num_connections}\n{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}",
        "tooltip-format-enumerate-connected-battery": "{device_alias}\t[ Û∞•â ]:{device_battery_percentage}%"
    },

    "memory": {
        "interval": 1,
        "format": "Û∞üú {}%",
        "tooltip": true,
        "tooltip-format": "Memory - {used:0.1f}GB used",
        "on-click": "gnome-system-monitor",
        "on-click-right": "alacritty -e htop"
    },

    "cpu": {
        "interval": 1,
        "format": "Ôãõ {usage}%",
        "on-click": "gnome-system-monitor"
    },

    "disk": {
        "interval": 1,
        "format": "Û∞ãä {percentage_used}%",
        "path": "/home",
        "format-alt": "Û∞ãä {percentage_used}%",
        "tooltip": true,
        "tooltip-format": "Disk - {used} used out of {total} on {path} ({percentage_used}%)"
    },

    "temperature": {
        "interval": 1,
        "hwmon-path": "/sys/class/hwmon/hwmon5/temp1_input",
        "thermal-zone": 0,
        "format": "Ôãâ {temperatureC}¬∞C",
        "critical-threshold": 80,
        "format-critical": "Ôãá {temperatureC}¬∞C",
        "on-click": "gnome-system-monitor"
    },

    "power-profiles-daemon": {
        "format": "{icon} Power-profile:{profile}",
        "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
        "tooltip": true,
        "format-icons": {
            "default": "ÔÉß",
            "performance": "ÓÇ∑",
            "balanced": "Ôâé",
            "power-saver": "ÔÅ¨"
        }
    },


    "battery": {
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 20
        },
        "format": "{icon} {capacity}%",
        "format-charging": "Û±êã{icon} {capacity}%",
        "format-plugged": "ÔÉß{icon}:{capacity}%",
        "format-alt": "{time} {icon}",
        //"format-icons": ["ÔâÑ ", "ÔâÉ ", "ÔâÇ ", "ÔâÅ ", "ÔâÄ "]
        "format-icons": ["Û∞Çé", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"]
    },

    "pulseaudio": {
        "format": "{icon} {volume}%",

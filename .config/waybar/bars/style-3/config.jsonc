        "format-source": "ÔÑ∞ {volume}%",
        "format-source-muted": "ÔÑ± Mute",
        "tooltip": false,
        //      "tooltip-format": "ÔÑ∞ at {volume}%"
    },

    "mpris": {
        "format": "{player_icon} {dynamic}",
        "format-paused": "{status_icon} <i>{dynamic}</i>",
        "player-icons": {
            "default": "‚è∏",
            "mpv": "üéµ"
        },
        "status-icons": {
            "paused": "‚ñ∂"
        },
        "interval": 1,
        "max-length": 60

    "custom/notification": {
        "tooltip": false,
        "format": "{}{icon}",
        "format-icons": {
            "notification": "ÔÉ≥<span foreground='#F7768E'><sup>ÓÆ¥</sup></span>",
            "none": "ÔÉ≥",
            "dnd-notification": "Ôá∂<span foreground='#F7768E'><sup>ÓÆ¥</sup></span>",
            "dnd-none": "Ôá∂",
            "inhibited-notification": "ÔÉ≥<span foreground='#F7768E'><sup>ÓÆ¥</sup></span>",
            "inhibited-none": "ÔÉ≥",
            "dnd-inhibited-notification": "Ôá∂<span foreground='#F7768E'><sup>ÓÆ¥</sup></span>",
            "dnd-inhibited-none": "Ôá∂"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "sleep 0.1 && swaync-client -swb",
        "on-click": "sleep 0.1 && swaync-client -t -sw",
        "on-click-right": "sleep 0.1 && swaync-client -d -sw",
        "escape": true
    },

    "hyprland/window": {
        "format": "Ôãê : {class}",
        "rewrite": {

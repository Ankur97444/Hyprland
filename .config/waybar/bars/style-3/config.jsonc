            "5": "5",
            "6": "6",
            "7": "7",
            "8": "8",
            "9": "9",
            "10": "0",
            // "default": "‚àô",
            // "empty": "‚àô",
            // "active": "Ôãë",
            // "special": "ÔÄÖ",
            // "urgent": "Ôû∫"
        },

        "persistent-workspaces": {
            "*": 8
        }
    },

    //"wlr/taskbar": {
    //"format": "{icon}",
    // "icon-size": 18,
    // "icon-theme": "Papirus",
    //"spacing": 0,
    //"tooltip-format": "{title}",
